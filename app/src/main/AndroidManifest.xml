<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.aitangba.scrollheadview">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".horizonscroll.HorizonScrollActivity"></activity>
        <activity android:name=".verticalscroll.VerticalScrollActivity"></activity>
        <activity android:name=".customswipe.CustomSwipeActivity"></activity>
        <activity android:name=".horizonscroll.HorizonScrollTestActivity"></activity>
        <activity android:name=".multiadapter.MultiAdapterActivity"></activity>
    </application>

</manifest>